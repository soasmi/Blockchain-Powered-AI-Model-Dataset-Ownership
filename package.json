{
  "name": "ai-asset-marketplace",
  "version": "1.0.0",
  "description": "Blockchain-powered platform for AI model, script, and dataset ownership and monetization",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:contracts && npm run test:frontend && npm run test:backend",
    "test:contracts": "cd contracts && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "deploy:contracts": "cd contracts && npm run deploy",
    "compile:contracts": "cd contracts && npm run compile"
  },
  "keywords": [
    "blockchain",
    "ai",
    "marketplace",
    "nft",
    "ethereum",
    "ipfs",
    "smart-contracts"
  ],
  "author": "AI Asset Marketplace Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "contracts",
    "frontend",
    "backend"
  ]
}